{
  "expo": {
    "name": "Free-go",
    "slug": "Free-go",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "@assets/images/freeGo.jpg",
    "scheme": "Free-go",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "@assets/images/freeGo.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "",
        "backgroundColor": "#ffffff"
      },
      "package": "com.hashual.freego"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": ""
    },
    "plugins": [
      "expo-router",
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "7e1a9bb5-d29c-480a-90d4-dbd03d090c81"
      }
    }
  }
}
