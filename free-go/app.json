{
  "expo": {
    "name": "Free-go",
    "slug": "Free-go",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icone.png",
    "scheme": "free-go",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/freego.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icone.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.hashual.freego"
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/icone.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      [
        "expo-notifications",
        {
          "sounds": [
            "./assets/sounds/goutte.wav",
            "./assets/sounds/gratte.wav" 
          ]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "2d584cf8-1cef-4fd9-b7f9-82a6f27e9797"
      }
    }
  }
}
